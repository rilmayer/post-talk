{
  "name": "line-post-page",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run css -- -w --source-map true & npm run js -- -w -s",
    "build": "rm -rf dist && npm run css && npm run js",
    "css": "node-sass src -o dist --output-style compressed",
    "js": "babel src -d dist --compact=true --presets=env --no-comments"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rilmayer/line-post"
  },
  "author": "PetaPeta",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^4.9.3"
  }
}
